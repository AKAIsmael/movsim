<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE SCENARIO SYSTEM "sim/multiModelTrafficSimulatorInput.dtd">
<!-- (c) Martin Treiber, Arne Kesting 2010 -->
<SCENARIO>
    <VEHICLES>
        <VEHICLE label="NEWELL1" length="8" b_max="100000000">
            <LONGITUDINAL_MODEL>
                <NEWELL v0="15" s0="2" dt="1.0" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
        <VEHICLE label="Obstacle" length="0" b_max="20">
            <LONGITUDINAL_MODEL>
                <NEWELL v0="0" s0="2" dt="1.0" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
    </VEHICLES>
    <SIMULATION dt="1.0" duration="360" fixed_seed="true" seed="42">
        <ROAD id="1" length="20000" lanes="1">
            <TRAFFIC_COMPOSITION write_fund_diagrams="true">
                <VEHICLE_TYPE label="NEWELL1" fraction="1" />
                <VEHICLE_TYPE label="Obstacle" fraction="0" />
            </TRAFFIC_COMPOSITION>
            <INITIAL_CONDITIONS>
                <IC_MICRO x="10" v="0" label="NEWELL1" />
                <IC_MICRO x="20" v="0" label="NEWELL1" />
                <IC_MICRO x="30" v="0" label="NEWELL1" />
                <IC_MICRO x="40" v="0" label="NEWELL1" />
                <IC_MICRO x="50" v="0" label="NEWELL1" />
                <IC_MICRO x="60" v="0" label="NEWELL1" />
                <IC_MICRO x="70" v="0" label="NEWELL1" />
                <IC_MICRO x="80" v="0" label="NEWELL1" />
                <IC_MICRO x="90" v="0" label="NEWELL1" />
                <IC_MICRO x="100" v="0" label="NEWELL1" />
                <IC_MICRO x="110" v="0" label="NEWELL1" />
                <IC_MICRO x="120" v="0" label="NEWELL1" />
                <IC_MICRO x="130" v="0" label="NEWELL1" />
                <IC_MICRO x="140" v="0" label="NEWELL1" />
                <IC_MICRO x="150" v="0" label="NEWELL1" />
                <IC_MICRO x="160" v="0" label="NEWELL1" />
                <IC_MICRO x="170" v="0" label="NEWELL1" />
                <IC_MICRO x="180" v="0" label="NEWELL1" />
                <IC_MICRO x="190" v="0" label="NEWELL1" />
                <IC_MICRO x="200" v="0" label="NEWELL1" />
                <IC_MICRO x="210" v="0" label="NEWELL1" />
                <IC_MICRO x="220" v="0" label="NEWELL1" />
                <IC_MICRO x="230" v="0" label="NEWELL1" />
                <IC_MICRO x="240" v="0" label="NEWELL1" />
                <IC_MICRO x="250" v="0" label="NEWELL1" />
                <IC_MICRO x="260" v="0" label="NEWELL1" />
                <!-- obstacle shifts floating car numbers -->
                <IC_MICRO x="1000" v="0" label="Obstacle" />
            </INITIAL_CONDITIONS>
            <TRAFFIC_SOURCE>
            </TRAFFIC_SOURCE>
            <TRAFFIC_SINK />
            <FLOW_CONSERVING_INHOMOGENEITIES>
            </FLOW_CONSERVING_INHOMOGENEITIES>
            <SPEED_LIMITS>
            </SPEED_LIMITS>
            <RAMPS>
            </RAMPS>
            <TRAFFIC_LIGHTS>
                <!-- <TRAFFIC_LIGHT x="1000" green_time="30" green_red_time="3" red_time="200" 
                    red_green_time="2" phase_shift="0" /> -->
            </TRAFFIC_LIGHTS>
            <DETECTORS />
        </ROAD>
        <OUTPUT>
            <FLOATING_CAR_DATA n_dt="1" perc_out="0" dn="0">
                    <!-- FC 1 is obstacle -->
                    <FC number="1" />
                    <FC number="2" />
                    <FC number="6" />
                    <FC number="11" />
                    <FC number="16" />
                    <FC number="21" />
                </FLOATING_CAR_DATA>
            </OUTPUT>
    </SIMULATION>
</SCENARIO>
