##  1=green, 2=green_red (yellow1), 3=red, 4=red_green (yellow2)
set style line 16 pt 5 ps 1 lc rgb "green"
set style line 17 pt 5 ps 1 lc rgb "#ffa500"
set style line 18 pt 5 ps 1 lc rgb "red"
set style line 19 pt 5 ps 1 lc rgb "yellow"

set term post eps enhanced color solid "Helvetica" 26

########################################################


set size 1,1
deltaTL(x,y, pos) = (abs(1.*x-1.*y)<0.1) ? pos: -1000

set xlabel "Time (s)"
set ylabel "Position (m)"
unset key
set pm3d map

set yrange [0:]
set out "./figs/test_trafficlights_IDM.trajectories.eps"
splot\
 "test_trafficlights_IDM.trafficlights_log" u ($1):(deltaTL($2,0,$3)):(1) t "" w p ls 16,\
