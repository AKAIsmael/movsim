<SCENARIO network_filename="startStop.xodr">
    <FUEL_CONSUMPTION>
       <CONSUMPTION_MODEL label="carConsumption" >
       <CAR mass="1500" cross_section_surface="2.2" cd_value="0.32" electric_power="3000" const_friction="0.015" v_friction="0" dynamic_tyre_radius="0.31" />
       <ENGINE max_power_kW="90" cylinder_vol_l="1.4" idle_cons_rate_linvh="0.8" cspec_min_g_per_kwh="280" pe_min_bar="1" pe_max_bar="19"
             idle_rotation_rate_invmin="1000" max_rotation_rate_invmin="6000" >
       <GEAR_RATIOS>
         <GEAR_RATIO phi="16.7" />
         <GEAR_RATIO phi="10.1" />
         <GEAR_RATIO phi="6.8" />
         <GEAR_RATIO phi="5.0" />
         <GEAR_RATIO phi="3.8" />
         <GEAR_RATIO phi="3.1" />
         <GEAR_RATIO phi="2.6" />
       </GEAR_RATIOS>
       </ENGINE>
       </CONSUMPTION_MODEL>
    </FUEL_CONSUMPTION>
    <VEHICLES>
        <VEHICLE label="ACC1" length="8" b_max="9" consumption="carConsumption">
            <LONGITUDINAL_MODEL>
                <ACC v0="15" T="1.2" s0="2" s1="0" delta="4" a="1.5" b="1.5" coolness="1" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
        <VEHICLE label="Obstacle" length="0">
            <LONGITUDINAL_MODEL>
                <IDM v0="0" T="1" s0="2" a="1.0" b="1.5"/>
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL/>
        </VEHICLE>
    </VEHICLES>
    <SIMULATION dt="0.2" duration="360" seed="42">
        <TRAFFIC_COMPOSITION write_fund_diagrams="true">
            <VEHICLE_TYPE label="ACC1" fraction="1" />
        </TRAFFIC_COMPOSITION>
        <ROAD id="1">
            <INITIAL_CONDITIONS>
                <IC_MICRO  x="10" v="0" />
                <IC_MICRO  x="20" v="0" />
                <IC_MICRO  x="30" v="0" />
                <IC_MICRO  x="40" v="0" />
                <IC_MICRO  x="50" v="0" />
                <IC_MICRO  x="60" v="0" />
                <IC_MICRO  x="70" v="0" />
                <IC_MICRO  x="80" v="0" />
                <IC_MICRO  x="90" v="0" />
                <IC_MICRO x="100" v="0" />
                <IC_MICRO x="110" v="0" />
                <IC_MICRO x="120" v="0" />
                <IC_MICRO x="130" v="0" />
                <IC_MICRO x="140" v="0" />
                <IC_MICRO x="150" v="0" />
                <IC_MICRO x="160" v="0" />
                <IC_MICRO x="170" v="0" />
                <IC_MICRO x="180" v="0" />
                <IC_MICRO x="190" v="0" />
                <IC_MICRO x="200" v="0" />
                <IC_MICRO x="210" v="0" />
                <IC_MICRO x="220" v="0" />
                <IC_MICRO x="230" v="0" />
                <IC_MICRO x="240" v="0" />
                <IC_MICRO x="250" v="0" />
                <IC_MICRO x="260" v="0" />
            </INITIAL_CONDITIONS>
            <TRAFFIC_SOURCE />
            <TRAFFIC_LIGHTS>
                <TRAFFIC_LIGHT x="1000" green_time="30" green_red_time="3" red_time="200" red_green_time="2" phase_shift="0" />
            </TRAFFIC_LIGHTS>
        </ROAD>
        <OUTPUT>
            <FLOATING_CAR_DATA n_dt="5" perc_out="0" dn="0">
                <FC number="1" />
                <FC number="5" />
                <FC number="10" />
                <FC number="15" />
                <FC number="20" />
            </FLOATING_CAR_DATA>
        </OUTPUT>
    </SIMULATION>
</SCENARIO>
