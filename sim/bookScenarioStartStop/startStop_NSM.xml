<SCENARIO network_filename="startStop.xodr">
    <VEHICLES>
           <!-- vehicle length ==> cell units -->
        <VEHICLE label="NSM1" length="1">
            <!-- b_max not limited -->
            <LONGITUDINAL_MODEL>
                <!-- BARL variant : p0 != p possible -->
                <NSM v0="2" p_slowdown="0.1" p_slow_start="0.1" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
        <VEHICLE label="Obstacle" length="0">
            <LONGITUDINAL_MODEL>
                <IDM v0="0" T="1" s0="2" a="1.0" b="1.5"/>
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL/>
        </VEHICLE>
    </VEHICLES>
    <SIMULATION dt="1" duration="360" seed="42">
        <TRAFFIC_COMPOSITION write_fund_diagrams="true">
            <VEHICLE_TYPE label="NSM1" fraction="1" />
        </TRAFFIC_COMPOSITION>
        <ROAD id="1">
            <INITIAL_CONDITIONS>
                <IC_MICRO x="1" v="0" label="NSM1" />
                <IC_MICRO x="2" v="0" label="NSM1" />
                <IC_MICRO x="3" v="0" label="NSM1" />
                <IC_MICRO x="4" v="0" label="NSM1" />
                <IC_MICRO x="5" v="0" label="NSM1" />
                <IC_MICRO x="6" v="0" label="NSM1" />
                <IC_MICRO x="7" v="0" label="NSM1" />
                <IC_MICRO x="8" v="0" label="NSM1" />
                <IC_MICRO x="9" v="0" label="NSM1" />
                <IC_MICRO x="10" v="0" label="NSM1" />
                <IC_MICRO x="11" v="0" label="NSM1" />
                <IC_MICRO x="12" v="0" label="NSM1" />
                <IC_MICRO x="13" v="0" label="NSM1" />
                <IC_MICRO x="14" v="0" label="NSM1" />
                <IC_MICRO x="15" v="0" label="NSM1" />
                <IC_MICRO x="16" v="0" label="NSM1" />
                <IC_MICRO x="17" v="0" label="NSM1" />
                <IC_MICRO x="18" v="0" label="NSM1" />
                <IC_MICRO x="19" v="0" label="NSM1" />
                <IC_MICRO x="20" v="0" label="NSM1" />
                <IC_MICRO x="21" v="0" label="NSM1" />
                <IC_MICRO x="22" v="0" label="NSM1" />
                <IC_MICRO x="23" v="0" label="NSM1" />
                <IC_MICRO x="24" v="0" label="NSM1" />
                <IC_MICRO x="25" v="0" label="NSM1" />
                <IC_MICRO x="26" v="0" label="NSM1" />
            </INITIAL_CONDITIONS>
            <TRAFFIC_SOURCE />
            <TRAFFIC_LIGHTS>
                <TRAFFIC_LIGHT x="100" green_time="30" green_red_time="3" red_time="200" red_green_time="2" phase_shift="0" />
            </TRAFFIC_LIGHTS>
        </ROAD>
        <OUTPUT>
            <FLOATING_CAR_DATA n_dt="5" perc_out="0" dn="0">
                <FC number="1" />
                <FC number="5" />
                <FC number="10" />
                <FC number="15" />
                <FC number="20" />
            </FLOATING_CAR_DATA>
        </OUTPUT>
    </SIMULATION>
</SCENARIO>
