<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE SCENARIO SYSTEM "multiModelTrafficSimulatorInput.dtd">

<!-- (c) Martin Treiber, Arne Kesting 2010 -->
<SCENARIO network_filename="vasa.xodr">
    <VEHICLES>
        <VEHICLE label="CCS1" length="2" width="1" b_max="9">
            <LONGITUDINAL_MODEL>
                <CCS mass="80" A="1" cw="0.7" friction="0.02" T="1.2" s0="1" p0="200" v_c="8" p_herringbone="150" v_c_herringbone="1.5" b="1" b_maximal="2" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL eur_rules="false">
                <MOBIL b_safe="2.0" s_min="1.0" threshold="0.05" bias_right="0.0" politeness="0.0" />
            </LANE_CHANGE_MODEL>
        </VEHICLE>
        <VEHICLE label="CCS2" length="2" width="1" b_max="9">
            <LONGITUDINAL_MODEL>
                <CCS mass="84" A="1" cw="0.7" friction="0.02" T="1.2" s0="1" p0="150" v_c="5" p_herringbone="100" v_c_herringbone="1.5" b="1" b_maximal="2"/>
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL eur_rules="false">
                <MOBIL b_safe="2.0" s_min="1.0" threshold="0.2" bias_right="0.0" politeness="0.0" />
            </LANE_CHANGE_MODEL>
        </VEHICLE>
    </VEHICLES>
    <SIMULATION dt="0.2" duration="1000" fixed_seed="true" seed="42" crash_exit="false">
        <TRAFFIC_COMPOSITION write_fund_diagrams="false">
            <VEHICLE_TYPE label="CCS1" fraction="0.3" relative_v0_randomization="0.1" />
            <VEHICLE_TYPE label="CCS2" fraction="0.7" relative_v0_randomization="0.2" />
        </TRAFFIC_COMPOSITION>
        <ROAD id="1">
            <INITIAL_CONDITIONS>
                <IC_MACRO x="1" rho_per_km="330" v="0" />
            </INITIAL_CONDITIONS>
        </ROAD>
        <OUTPUT />
    </SIMULATION>
</SCENARIO>

